    "compilerOptions": {
        "target": "es6",
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"],
            "@c/*": ["src/components/*"],
            "@v/*": ["src/views/*"],
            "@api/*": ["src/api/*"]
